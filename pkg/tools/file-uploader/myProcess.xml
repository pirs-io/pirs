                </layout>
            </dataRef>
            <dataRef>
                <id>document_type</id>
                <logic>
                    <behavior>editable</behavior>
                </logic>
                <layout>
                    <x>2</x>
                    <y>2</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
            <dataRef>
                <id>note</id>
                <logic>
                    <behavior>editable</behavior>
                </logic>
                <layout>
                    <x>2</x>
                    <y>1</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
        </dataGroup>
        <event type="finish">
            <id>t5_finish</id>
            <actions phase="post">
                <action>status: f.status;

                    change status value { return "Document reworked and waiting for decision" }
                </action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>t6</id>
        <x>620</x>
        <y>100</y>
        <layout>
            <offset>0</offset>
        </layout>
        <label>Check approval message</label>
        <roleRef>
            <id>user</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
        <dataGroup>
            <id>DataGroup_0</id>
            <dataRef>
                <id>approve_note</id>
                <logic>
                    <behavior>visible</behavior>
                </logic>
                <layout>
                    <x>0</x>
                    <y>0</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
        </dataGroup>
    </transition>
    <transition>
        <id>t7</id>
        <x>900</x>
        <y>500</y>
        <layout>
            <offset>0</offset>
        </layout>
        <label>Scrap document</label>
        <icon>delete_forever</icon>
        <roleRef>
            <id>user</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
        <event type="finish">
            <id>t7_finish</id>
            <actions phase="post">
                <action>status: f.status;

                    change status value { return "Document scrapped" }
                </action>
            </actions>
        </event>
