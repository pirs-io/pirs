<?xml version="1.0" encoding="UTF-8"?>
<document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="https://modeler.netgrif.com/petriflow_schema.xsd">
    <id>new_model</id>
    <initials>NEW</initials>
    <title>New Model</title>
    <transitionRole>false</transitionRole>
    <!-- TRANSACTIONS -->
    <!-- ROLES -->
    <role>
        <id>user</id>
        <title>User</title>
    </role>
    <role>
        <id>approver</id>
        <title>Approver</title>
    </role>
    <!-- PROCESS ROLE REFS -->
    <!-- PROCESS USER REFS -->
    <!-- DATA -->
    <data type="text" immediate="true">
        <id>status</id>
        <title>Status</title>
        <init>Document to be completed</init>
    </data>
    <data type="fileList">
        <id>files</id>
        <title>Files</title>
    </data>
    <data type="text">
        <id>note</id>
        <title>Notes</title>
        <values>area</values>
    </data>
    <data type="enumeration_map" immediate="true">
        <id>document_type</id>
        <title>Stage</title>
        <options>
            <option key="draft">Draft</option>
            <option key="internal">Ready for internal use</option>
            <option key="final">Ready for client</option>
        </options>
    </data>
    <data type="text">
        <id>approve_note</id>
        <title>Appproval note</title>
        <values>area</values>
    </data>
    <data type="text">
        <id>text_2</id>
        <title>Reason for declining</title>
        <values>area</values>
    </data>
    <!-- I18NS -->
    <!-- TRANSITIONS -->
    <transition>
        <id>t1</id>
        <x>500</x>
        <y>380</y>
        <layout>
            <offset>0</offset>
        </layout>
        <label>Enter new document</label>
        <icon>upload_file</icon>
        <assignPolicy>auto</assignPolicy>
        <roleRef>
            <id>user</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
        <dataGroup>
            <id>DataGroup_0</id>
            <dataRef>
                <id>files</id>
                <logic>
                    <behavior>editable</behavior>
                    <behavior>required</behavior>
                </logic>
                <layout>
                    <x>0</x>
                    <y>0</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance></appearance>
                </layout>
            </dataRef>
            <dataRef>
                <id>note</id>
                <logic>
                    <behavior>editable</behavior>
                </logic>
                <layout>
                    <x>2</x>
                    <y>0</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
            <dataRef>
                <id>document_type</id>
                <logic>
                    <behavior>editable</behavior>
                    <behavior>required</behavior>
                </logic>
                <layout>
                    <x>0</x>
                    <y>1</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
        </dataGroup>
        <event type="finish">
            <id>t1_finish</id>
            <actions phase="post">
                <action>status: f.status;

                    change status value { return "Waiting for decision" }
                </action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>t2</id>
        <x>500</x>
        <y>180</y>
        <layout>
            <offset>0</offset>
        </layout>
        <label>Check data</label>
        <assignPolicy>auto</assignPolicy>
        <roleRef>
            <id>user</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
        <roleRef>
            <id>approver</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
        <dataGroup>
            <id>DataGroup_0</id>
            <dataRef>
                <id>status</id>
                <logic>
                    <behavior>visible</behavior>
                </logic>
                <layout>
                    <x>0</x>
                    <y>0</y>
                    <rows>1</rows>
                    <cols>4</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
            <dataRef>
                <id>files</id>
                <logic>
                    <behavior>visible</behavior>
                </logic>
                <layout>
                    <x>0</x>
                    <y>1</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance></appearance>
                </layout>
            </dataRef>
            <dataRef>
                <id>note</id>
                <logic>
                    <behavior>visible</behavior>
                </logic>
                <layout>
                    <x>2</x>
                    <y>1</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
            <dataRef>
                <id>document_type</id>
                <logic>
                    <behavior>visible</behavior>
                </logic>
                <layout>
                    <x>2</x>
                    <y>2</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
        </dataGroup>
    </transition>
    <transition>
        <id>t3</id>
        <x>620</x>
        <y>300</y>
        <layout>
            <offset>0</offset>
        </layout>
        <label>Approve</label>
        <icon>check_circle_outline</icon>
        <assignPolicy>auto</assignPolicy>
        <roleRef>
            <id>approver</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
        <dataGroup>
            <id>DataGroup_0</id>
            <dataRef>
                <id>approve_note</id>
                <logic>
                    <behavior>editable</behavior>
                </logic>
                <layout>
                    <x>0</x>
                    <y>0</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
        </dataGroup>
        <event type="finish">
            <id>t3_finish</id>
            <actions phase="post">
                <action>status: f.status;

                    change status value { return "Document accepted" }
                </action>
            </actions>
        </event>
    </transition>
    <transition>
        <id>t4</id>
        <x>620</x>
        <y>500</y>
        <layout>
            <offset>0</offset>
        </layout>
        <label>Decline</label>
        <icon>highlight_off</icon>
        <assignPolicy>auto</assignPolicy>
        <roleRef>
            <id>approver</id>
            <logic>
                <perform>true</perform>
            </logic>
        </roleRef>
        <dataGroup>
            <id>DataGroup_0</id>
            <dataRef>
                <id>text_2</id>
                <logic>
                    <behavior>editable</behavior>
                </logic>
                <layout>
                    <x>0</x>
                    <y>0</y>
                    <rows>1</rows>
                    <cols>2</cols>
                    <offset>0</offset>
                    <template>material</template>
                    <appearance>outline</appearance>
                </layout>
            </dataRef>
        </dataGroup>
        <event type="finish">
            <id>t4_finish</id>
            <actions phase="post">
                <action>status: f.status;

                    change status value { return "Document declined" }
                </action>
            </actions>
        </event>
